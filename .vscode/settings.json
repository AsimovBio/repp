{
  "go.useLanguageServer": true,
  "python.pythonPath": "/usr/local/opt/python@2/bin/python2.7",
  "python.formatting.provider": "black",
  "git.ignoreLimitWarning": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "assets/addgene/repo": true
  },
  "code-runner.executorMap": {
    "python": "python3"
  }
}
